# Environment variables for LLM Router with Aether Shared Infrastructure
# Copy this file to .env.aether-shared and update with your actual values

# Required: Provider API Keys
OPENAI_API_KEY=sk-test-openai-key-demo
ANTHROPIC_API_KEY=

# Optional: Provider Configuration
# OPENAI_ORG_ID=org-your-organization-id
# OPENAI_BASE_URL=https://api.openai.com/v1
# ANTHROPIC_BASE_URL=https://api.anthropic.com

# Optional: Shared Infrastructure Configuration
# These use the default values from aether-shared, override if needed

# MinIO Configuration (uses shared MinIO service)
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin123

# PostgreSQL Configuration (uses shared PostgreSQL service)
POSTGRES_USER=tasuser
POSTGRES_PASSWORD=taspassword

# Keycloak Configuration (uses shared Keycloak service)  
KEYCLOAK_ADMIN_PASSWORD=admin123
KEYCLOAK_DB_PASSWORD=keycloak123

# Grafana Configuration (uses shared Grafana service)
GRAFANA_PASSWORD=admin123

# Redis Configuration (uses shared Redis service)
REDIS_PASSWORD=

# Environment
ENVIRONMENT=development

# LLM Router Specific Configuration
LLM_ROUTER_PORT=8080
LLM_ROUTER_LOG_LEVEL=debug
LLM_ROUTER_LOG_FORMAT=json
LLM_ROUTER_DEFAULT_STRATEGY=cost_optimized